<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CarRepairAppointment.
    -->
    <changeSet id="20240214140610-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="car_id"
                                 baseTableName="car_repair_appointment"
                                 constraintName="fk_car_repair_appointment__car_id"
                                 referencedColumnNames="id"
                                 referencedTableName="car"
                                 />

        <addForeignKeyConstraint baseColumnNames="car_service_id"
                                 baseTableName="car_repair_appointment"
                                 constraintName="fk_car_repair_appointment__car_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="car_service"
                                 />
    </changeSet>
</databaseChangeLog>
